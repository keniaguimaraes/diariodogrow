<p id="notice"><%= notice %></p>

<h1>Tipo Sementes</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_sementes.each do |tipo_semente| %>
      <tr>
        <td><%= tipo_semente.descricao %></td>
        <td><%= link_to 'Show', tipo_semente %></td>
        <td><%= link_to 'Edit', edit_tipo_semente_path(tipo_semente) %></td>
        <td><%= link_to 'Destroy', tipo_semente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Semente', new_tipo_semente_path %>
